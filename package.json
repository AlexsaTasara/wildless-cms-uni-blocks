    "cssnano": "^5.1.7",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "prettier": "2.6.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3"
  }
